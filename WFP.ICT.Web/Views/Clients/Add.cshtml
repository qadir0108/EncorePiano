                                                            <div class="input-icon">
                                                                @Html.EditorFor(model => model.Client.Email, new { htmlAttributes = new { @class = "form-control" } })
                                                                <label>Email Account<span class="required">*</span></label>
                                                                @Html.ValidationMessageFor(model => model.Client.Email, null, new { @class = "help-block help-block-error" })
                                                                <span class="help-block">Enter Email Account</span>
                                                                <i class="fa fa-user"></i>
                                                            </div>
