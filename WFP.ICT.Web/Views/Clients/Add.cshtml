                                                                @Html.ValidationMessageFor(model => model.Client.PhoneNumber, null, new { @class = "help-block help-block-error" })
                                                                <span class="help-block">Enter Phone Number</span>
                                                                <i class="fa fa-user"></i>
                                                            </div>
                                                        </div>
                                                    </div>

                                                </div>

                                                <div class="row">

                                                    <div class="col-md-12">
                                                        <div class="form-group form-md-line-input has-success">
                                                            <div class="input-icon">
                                                                @Html.EditorFor(model => model.Client.AccountCode, new { htmlAttributes = new { @class = "form-control" } })
                                                                <label>Account Code<span class="required">*</span></label>
                                                                @Html.ValidationMessageFor(model => model.Client.AccountCode, null, new { @class = "help-block help-block-error" })
                                                                <span class="help-block">Enter Account Code</span>
                                                                <i class="fa fa-user"></i>
