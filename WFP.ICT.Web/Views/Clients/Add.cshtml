                                                                @Html.ValidationMessageFor(model => model.PostCode, null, new { @class = "help-block help-block-error" })
                                                                <span class="help-block">Zip Code</span>
                                                                <i class="fa fa-user"></i>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                        <div class="map-container-address">
                                                            <div class="label label-danger visible-ie8"> Not supported in Internet Explorer 8 </div>
                                                            <div id="gmap_location" class="gmaps"> </div>
                                                        </div>
                                                    @Html.ValidationMessageFor(model => model.lng, null, new { @class = "help-block help-block-error" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    @Html.HiddenFor(x => x.Id, htmlAttributes: new { id = "AddressId" })

                                    @Html.HiddenFor(x => x.lat, htmlAttributes: new { id = "AddressLat" })

                                    @Html.HiddenFor(x => x.lng, htmlAttributes: new { id = "AddressLng" })


                      
                                    <!-- END SAMPLE FORM PORTLET-->
                                </div>
                            </div>
                          
                        </div>
         
            }
     






