            @using (Ajax.BeginForm("Save", "Clients", null, new AjaxOptions { HttpMethod = "POST", OnSuccess = "AddressConfigs.initAddSuccess(data,status)" }, new
            {
                @id = "frm",
            }))
            {

                        <div class="portlet-body">

                            <div class="row">
                                <div class="col-md-6 ">
                                    <!-- BEGIN SAMPLE FORM PORTLET-->

                                    <div class="portlet light bordered">
                                        <div class="portlet-title">
                                            <div class="caption font-red-sunglo">
                                                <span class="caption-subject bold uppercase"> New Client</span>
                                            </div>
                                            <div class="actions">
                                                <a class="btn btn-circle btn-icon-only btn-default fullscreen" href="javascript:;" data-original-title="" title=""> </a>
                                            </div>
                                        </div>
                                        <div class="portlet-body form">
                                            <div class="form-body">
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div class="form-group form-md-line-input has-success">
                                                            <div class="input-icon">
                                                                @Html.DropDownListFor(model => model.Client.Type, TempData["ClientTypes"] as SelectList, htmlAttributes: new { @class = "form-control" })
                                                                <label>Client Type<span class="required">*</span></label>
                                                                @Html.ValidationMessageFor(model => model.Client.Type, null, new { @class = "help-block help-block-error" })
                                                                <span class="help-block">Enter Client Type</span>
                                                                <i class="fa fa-user"></i>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-12">
                                                        <div class="form-group form-md-line-input has-success">
                                                            <div class="input-icon">
                                                                @Html.EditorFor(model => model.Client.Name, new { htmlAttributes = new { @class = "form-control" } })
                                                                <label>First Name <span class="required">*</span></label>
                                                                @Html.ValidationMessageFor(model => model.Client.Name, null, new { @class = "help-block help-block-error" })
                                                                <span class="help-block">Enter First Name</span>
                                                                <i class="fa fa-user"></i>
                                                            </div>
                                                        </div>
                                                    </div>
