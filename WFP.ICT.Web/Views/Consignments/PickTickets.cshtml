@model List<WFP.ICT.Web.Models.ConsigmentVm>
@{
    ViewBag.Title = "PickTickets";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
    <!-- BEGIN CONTENT -->
    <div class="page-content-wrapper">
        <!-- BEGIN CONTENT BODY -->
        <div class="page-content">
            <!-- BEGIN PAGE HEAD-->
            <div class="page-head">
                <!-- BEGIN PAGE TITLE -->
                <div class="page-title">
                    <h1>
                        Pick Tickets
                        <small></small>
                    </h1>
                </div>
                <!-- END PAGE TITLE -->
            </div>
            <!-- END PAGE HEAD-->
            <!-- BEGIN PAGE BREADCRUMB -->
            <ul class="page-breadcrumb breadcrumb">
                <li>
                    <a href="index.html">Home</a>
                    <i class="fa fa-circle"></i>
                </li>
                <li>
                    <a href="#">Pick Tickets</a>
                    <i class="fa fa-circle"></i>
                </li>
                <li>
                    <span class="active"></span>
                </li>
            </ul>
            <!-- END PAGE BREADCRUMB -->
            @if (TempData["Success"] != null)
            {
                <div class="custom-alerts alert alert-success fade in">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true"></button>
                    <i class="fa-lg fa fa-check"></i>@TempData["Success"]
                </div>
            }
            @if (TempData["Error"] != null)
            {
                <div class="custom-alerts alert alert-danger fade in">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true"></button>
                    <i class="fa-lg fa fa-warning"></i>@TempData["Error"]
                </div>
            }
            <!-- BEGIN PAGE BASE CONTENT -->
          @using (Html.BeginForm("Generate", "Consignments"))
          {
              @Html.AntiForgeryToken()
              <div class="row">
                  <div class="col-md-12">
                      <!-- BEGIN EXAMPLE TABLE PORTLET-->
                      <div class="portlet box red">
                          <div class="portlet-title">
                              <div class="caption">
                                  <i class="fa fa-globe"></i> Pick Tickets
                              </div>
                              <div class="tools">
                                  <input type="submit" id="btnPickTickets" class="btn green" value="Pick Tickets"/>
                              </div>
                              <div class="actions">
                              </div>
                          </div>
                          <div class="portlet-body">
                              <table class="table table-striped table-bordered table-hover dt-responsive" width="100%">
                                  <thead>
                                  <tr>
                                      <th class="all">Cons Number</th>
                                      <th class="all">Order Number</th>
                                      <th class="all">Creation Date</th>
                                      <th class="all">Warehouse</th>
                                      <th class="all">Vehicle</th>
                                      <th class="all">Driver</th>
                                      @*
        min-phone-l min-tablet <th class="desktop"></th>
    <th class="none"></th>*@
                                  </tr>
                                  </thead>
                                  <tbody>
                                  @foreach (var order in Model)
                                  {
                                      <tr>
                                          <td>@order.ConsignmentNumber</td>
                                          <td>@order.OrderNumber</td>
                                          <td>@order.CreatedAt.ToString()</td>
                                          <td>@order.StartWarehouseName</td>
                                          <td>@order.VehicleName</td>
                                          <td>@order.DriverName</td>
                                      </tr>
                                  }
                                  </tbody>
                              </table>
                          </div>
                      </div>
                      <!-- END EXAMPLE TABLE PORTLET-->
                  </div>
              </div>
          }
            <!-- END PAGE BASE CONTENT -->
        </div>
        <!-- END CONTENT BODY -->
    </div>
    <!-- END CONTENT -->
<script>

    var wfp = wfp || {};

    $(function () {
        $('#gmap_routes_start').click(function (e) {

        });
    });
</script>